<div id='projects'>
  <h2>Projects</h2>
  <ul>
    <% @projects.each_with_index do |project, index| %>
      <li>
        <ul>
          <li>
            <%=image_tag project.photo.url(:thumb) %>
          </li>
          <li>
            <%= project.title %>
          </li>
          <li>
            <%= project.description.html_safe %>
          </li>
        </ul>
      </li>

    <%end%>
  </ul>

</div>
